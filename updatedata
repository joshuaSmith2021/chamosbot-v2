#!/bin/bash
echo ---------------------
echo Changing Directory...
cd /home/pi/chamosbot
echo Fetching new data for the hour... 
python3 hypixel.py parcerx gl4cier_fist red_lightning9 ronansfire catwing37 gamerboy80 purpled UPDATE
echo Creating new today and twoday files...
python3 hypixel.py BUILD
echo Deleting old data files...
sudo python3 hypixel.py CLEAN
echo Changing directory...
cd /home/pi/hypixel-player-data
echo Staging change for commit...
git add . 
echo Committing change...
git commit -m "Update data"
echo Pushing data to remote
git push
echo ---------------------
